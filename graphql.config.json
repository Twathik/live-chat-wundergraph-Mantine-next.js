{
  "projects": {
    "app": {
      "name": "app",
      "schemaPath": "packages/wunderChat/generated/wundergraph.app.schema.graphql",
      "extensions": {
        "endpoints": {
          "app": {
            "introspect": false,
            "url": "http://localhost:9991/app/main/graphql",
            "headers": {
              "user-agent": "WunderGraph Client"
            }
          }
        }
      }
    }
  }
}